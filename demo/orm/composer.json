        "update": "php bin/doctrine orm:schema-tool:update --force"
    },
    "require": {
        "doctrine/orm": "^3.1",
        "symfony/cache": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "Glodie\\Orm\\": "src/"
        }
    },
